{
  "name": "f1-world-champions-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:src": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint . --fix",
    "lint:src:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "format:config": "npx prettier --find-config-path src/App.tsx",
    "lint:format": "npm run lint && npm run format:check",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ci": "vitest run --coverage --reporter=verbose",
    "test:ui": "vitest --ui",
    "generate:types": "openapi-typescript http://localhost:3000/api-docs-json -o src/types/api.ts",
    "commit": "cz",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-i18next": "^15.5.2",
    "react-router": "^7.6.1",
    "tailwindcss": "^4.1.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "openapi-typescript": "^7.8.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "overrides": {
    "acorn": "^8.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
